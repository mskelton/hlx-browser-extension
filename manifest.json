{
  "name": "Higher Level FX",
  "description": "Helpful utilities to manage your Higher Level FX VPS.",
  "version": "1.0",
  "manifest_version": 3,
  "web_accessible_resources": [
    {
      "matches": ["https://whmcs.higherlevelfx.com/*"],
      "resources": ["js/paste.js"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["https://whmcs.higherlevelfx.com/*"],
      "js": ["content-script.js"]
    }
  ],
  "icons": {
    "16": "/images/icon-16.png",
    "32": "/images/icon-32.png",
    "48": "/images/icon-48.png",
    "128": "/images/icon-128.png"
  }
}
